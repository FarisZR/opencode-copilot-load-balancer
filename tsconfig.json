{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "types": ["bun-types"],
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": ["dist", "node_modules", "reference"]
}
