{
  "name": "opencode-copilot-multi-auth",
  "version": "1.0.0",
  "description": "OpenCode plugin for multi-account GitHub Copilot load balancing",
  "author": {
    "name": "opencode",
    "email": "support@opencode.ai"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opencode-ai/opencode-copilot-multi-auth"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "bun-types": "latest",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.47.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@opencode-ai/plugin": "^0.15.30",
    "@opencode-ai/sdk": "^0.15.30",
    "zod": "^4.0.0"
  }
}
